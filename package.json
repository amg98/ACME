        "@types/bcryptjs": "^2.4.2",
        "@types/chai": "^4.2.15",
        "@types/cors": "^2.8.9",
        "@types/express": "^4.17.9",
        "@types/paypal-rest-sdk": "^1.7.5",
        "chai": "^4.3.0",
        "mocha": "^8.3.0",
        "nodemon": "^2.0.7",
