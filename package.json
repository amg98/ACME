{
    "name": "acme-explorer",
    "version": "1.0.0",
    "description": "Backend for ACME Explorer",
    "scripts": {
        "start": "node -r dotenv/config source/boot.js dotenv_config_path=.devel.env",
        "start:prod": "node -r dotenv/config source/boot.js dotenv_config_path=.prod.env",
        "test": "jest ./test -i --silent",
        "coverage": "npm test -- --coverage"
    },
    "author": "amg98@github.com, LorenRd@github.com, josperdom1@github.com, 1nt0@github.com",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-swagger-generator": "^1.1.17",
        "mongodb": "3.6.3",
        "mongoose": "5.11.15"
    },
    "devDependencies": {
        "@types/cors": "^2.8.9",
        "@types/express": "^4.17.9",
        "@types/jest": "^26.0.18",
        "@types/supertest": "^2.0.10",
        "jest": "^26.6.3",
        "supertest": "^6.0.1"
    },
    "jest": {
        "globalSetup": "./test/setup.js",
        "globalTeardown": "./test/teardown.js",
        "collectCoverageFrom": [
            "./source/**/*.js"
        ],
        "testEnvironment": "node"
    }
}
