        "express-swagger-generator": "^1.1.17",
        "firebase-admin": "^9.5.0",
        "mongodb": "3.6.3",
        "mongoose": "5.11.15",
        "nodemon": "^2.0.7",
        "paypal-rest-sdk": "^1.8.1",
        "randexp": "^0.5.3",
        "request-promise": "^4.2.6"
    },
